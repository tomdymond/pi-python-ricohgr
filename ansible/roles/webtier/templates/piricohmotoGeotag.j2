[program:piricohmotoGeotag]
command=nice /home/venvs/piricohmoto/bin/python {{ install_root }}/bin/piricohmotoGeotag.py
directory=/tmp
user=appuser
autostart=true
autorestart=true
startretries=3
stderr_logfile=/var/log/piricohmoto/piricohmotoGeotag.err.log
stdout_logfile=/var/log/piricohmoto/piricohmotoGeotag.out.log
